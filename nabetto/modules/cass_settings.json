{
    "global": {
        "default_region": "NA"
    },
    "RiotAPI": {
        "api_key": "RIOT_API_KEY",
        "limiting_share": 1.0,
        "request_error_handling": {
            "404": {
                "strategy": "throw"
            },
            "429": {
                "service": {
                    "strategy": "exponential_backoff",
                    "initial_backoff": 1.0,
                    "backoff_factor": 2.0,
                    "max_attempts": 4
                },
                "method": {
                    "strategy": "retry_from_headers",
                    "max_attempts": 5
                },
                "application": {
                    "strategy": "retry_from_headers",
                    "max_attempts": 5
                }
          },
          "500": {
              "strategy": "throw"
          },
          "503": {
              "strategy": "throw"
          },
          "timeout": {
              "strategy": "throw"
          }
        }
    },
    "logging": {
        "print_calls": true,
        "print_riot_api_key": false
    }
}