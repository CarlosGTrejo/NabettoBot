{
    "global": {
        "version_from_match": "9.24",
        "default_region": "NA"
    },
    "RiotAPI": {
        "api_key": "RGAPI-cd3d089d-50ce-4c49-8162-413af0eae2fc",
        "limiting_share": 1.0,
        "request_error_handling": {
            "404": {
                "strategy": "throw"
            },
            "429": {
                "service": {
                    "strategy": "exponential_backoff",
                    "initial_backoff": 1.0,
                    "backoff_factor": 2.0,
                    "max_attempts": 4
                },
                "method": {
                    "strategy": "retry_from_headers",
                    "max_attempts": 5
                },
                "application": {
                    "strategy": "retry_from_headers",
                    "max_attempts": 5
                }
            },
            "500": {
                "strategy": "throw"
            },
            "503": {
                "strategy": "throw"
            },
            "timeout": {
                "strategy": "throw"
            }
        }
    }
}